      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/Finova.png"
    },
    "extra": {
      "eas": {
        "projectId": "726d5c6c-3277-4a73-9c54-73b59c974c78"
