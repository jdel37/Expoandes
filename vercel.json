{
  "version": 2,
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": null,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
